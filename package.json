{
  "name": "check-external-dependencies",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --colors",
    "test:watch": "npm test -- --watch",
    "compile": "rimraf lib && concurrently npm:compile:src npm:compile:types",
    "compile:watch": "rimraf lib && concurrently npm:compile:src:watch npm:compile:types:watch",
    "compile:src": "swc src --out-dir lib",
    "compile:src:watch": "npm run compile:src -- --watch",
    "compile:types": "tsc",
    "compile:types:watch": "npm run compile:types -- --watch",
    "lint": "eslint src",
    "format": "prettier --write src",
    "prepare": "npm test && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/surikaterna/check-external-dependencies.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/surikaterna/check-external-dependencies/issues"
  },
  "homepage": "https://github.com/surikaterna/check-external-dependencies#readme",
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-surikat": "^4.0.4",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "prettier-config-surikaterna": "^1.0.1",
    "rimraf": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "axios": "^1.2.3"
  }
}
